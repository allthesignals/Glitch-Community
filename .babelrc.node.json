{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", { "targets": { "node": true }, "useBuiltIns": false }]
  ],
  "plugins": [
    "styled-components",
    "@babel/syntax-dynamic-import",
    ["module-resolver", { "alias": { "@sentry/browser": "@sentry/node" } }]
  ],
  "babelrc": false
}
