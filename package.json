{
  "name": "glitch-community-site",
  "version": "0.0.1",
  "description": "Find cool projects and people using Glitch",
  "keywords": [
    "express",
    "glitch",
    "node",
    "react"
  ],
  "repository": {
    "url": "https://github.com/FogCreek/Glitch-Community"
  },
  "license": "Apache License Version 2.0",
  "sideEffects": [
    "./src/polyfills.js",
    "./src/utils/sentry.js",
    "./src/utils/lever/lever.js",
    "./src/utils/lever/lever.styl"
  ],
  "scripts": {
    "build": "npx webpack",
    "cy:ci": "start-server-and-test start http://localhost:8000 'npx cypress run --record'",
    "cy:open": "npx cypress open",
    "cy:remix": "npx cypress open --config baseUrl=https://$1.glitch.me",
    "cy:run": "npx cypress run",
    "lint": "npm run lint:server && npm run lint:src",
    "lint:server": "eslint --config server/.eslintrc.server.js server shared webpack.config.js aliases.js",
    "lint:src": "eslint --config src/.eslintrc.js src",
    "prestart": "if if-env BUILD_TYPE=\"\"; then \n npm run watch:lint:server & \n fi & \n if if-env BUILD_TYPE=memory; then \n npm run watch:lint:server & \n fi & \n if if-env BUILD_TYPE=watcher; then \n npm run watch:webpack \n npm run watch:lint:server & \n fi",
    "start": "pm2 start ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "test": "mochapack",
    "watch:lint:server": "nodemon --watch server --watch shared --exec 'npm run --silent lint:server'",
    "watch:webpack": "npx webpack --watch --info-verbosity verbose",
    "test:debug:watch": "node --inspect --inspect-brk node_modules/.bin/mochapack --watch \"test/**/*\""
  },
